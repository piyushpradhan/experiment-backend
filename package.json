{
  "name": "showoff",
  "version": "1.0.0",
  "main": "index.ts",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:build": "tsc --build && pm2 start ecosystem.config.json --no-daemon",
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "dev": "concurrently --kill-others \"npm run compile:watch\" \"npm run pre:dev\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.8",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status": "^1.7.4",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@types/jest": "^29.5.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "node-mocks-http": "^1.15.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "**/*.{js,ts}": "npm run lint:fix"
  },
  "author": "mediocre"
}
